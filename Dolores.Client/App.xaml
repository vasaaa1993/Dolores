<Application x:Class="Dolores.Client.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Dolores.Client"
             xmlns:infrastructure="clr-namespace:Dolores.Client.Infrastructure">
<Application.Resources>
	<ResourceDictionary>
		<ResourceDictionary.MergedDictionaries>
			<ResourceDictionary>
				<infrastructure:Bootstrapper x:Key="Bootstrapper"/>
			</ResourceDictionary>
		</ResourceDictionary.MergedDictionaries>
	</ResourceDictionary>
</Application.Resources>
</Application>
